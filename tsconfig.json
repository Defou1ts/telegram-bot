{
	"include": ["src", "src/commands/base.command.ts"],
	"baseUrl": "./src",
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2016",
		"module": "CommonJS",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"strictPropertyInitialization": false,
		"skipLibCheck": true,
		"experimentalDecorators": true
	} 
}
